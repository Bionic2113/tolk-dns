{
    "name": "tolk_dns",
    "version": "0.0.1",
    "scripts": {
        "bp": "blueprint",
        "start": "blueprint run",
        "build": "blueprint build",
        "test": "jest --verbose",
        "release": "blueprint pack && npm publish --access public",
        "---------": "echo 'next tolk-bench'",
        "clear": "rm -f bench-snapshots/*.last.json",
        "test:all": "npm run clear && jest",
        "bench:all": "npm run clear && jest -t bench",
        "test:exact": "npm run clear && jest 07",
        "bench:exact": "npm run clear && jest 07 -t bench",
        "save": "node tests/bench-cli.js save",
        "show": "node tests/bench-cli.js show",
        "generate-readme": "node tests/bench-cli.js readme"
    },
    "devDependencies": {
        "@ton-community/func-js": "^0.10.0",
        "@ton/blueprint": "^0.40.0",
        "@ton/core": "^0.61.0",
        "@ton/crypto": "^3.3.0",
        "@ton/sandbox": "^0.39.0",
        "@ton/test-utils": "^0.9.0",
        "@ton/tolk-js": "=1.0.0",
        "@ton/ton": "^15.3.0",
        "@tact-lang/compiler": ">=1.6.13 <2.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.15.32",
        "ansi-colors": "^4.1.3",
        "jest": "^29.7.0",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
